<Window x:Class="UI.FirstTaskForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Title="FirstTaskForm" Height="467" Width="800">
    <Grid Height="450" Width="800">
        <TextBox x:Name="ParsingTextBox" HorizontalAlignment="Left" Margin="27,22,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="576" Height="387"/>
        <Button x:Name="StartButton" Content="Start" HorizontalAlignment="Left" Margin="663,263,0,0" VerticalAlignment="Top" Width="40" Height="20" Click="Button_Click"/>
        <Button x:Name="AbortButton" Content="Abort" HorizontalAlignment="Left" Margin="663,332,0,0" VerticalAlignment="Top" Width="40" Height="20" Click="AbortButton_Click"/>
        <Label Content="Start page" HorizontalAlignment="Left" Margin="651,22,0,0" VerticalAlignment="Top"/>
        <Label Content="End page" HorizontalAlignment="Left" Margin="651,112,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="StartPageTextBox" PreviewTextInput="StartPageTextBox_PreviewTextInput"  HorizontalAlignment="Left" Margin="623,59,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="EndPageTextBox" PreviewTextInput="EndPageTextBox_PreviewTextInput"  HorizontalAlignment="Left" Margin="623,149,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
